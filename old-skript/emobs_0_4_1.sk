#============================================
#				Mob defenitions
#============================================

function spawn_demon(loc: location):
	spawn a wither skeleton at {_loc}
	set name of the last spawned wither skeleton to "&4Demon"
	
	apply resistance 3 to the last spawned wither skeleton for 9999 minutes
	apply strength 4 to the last spawned wither skeleton for 9999 minutes
	apply health boost 100 to the last spawned wither skeleton for 9999 minutes
	
	set tool of last spawned wither skeleton to {wep_dscythe}
	
	set helmet of last spawned wither skeleton to {skull_demon}
	set chestplate of last spawned wither skeleton to leather chestplate with name "&cDemon Chestplate"
	dye last spawned wither skeleton's chestplate (73, 3, 0)
	set leggings of last spawned wither skeleton to leather leggings with name "&cDemon Leggings"
	dye last spawned wither skeleton's leggings (140, 5, 0)
	set boots of last spawned wither skeleton to leather boots with name "&cDemon Boots"
	dye last spawned wither skeleton's boots (73, 3, 0)

function spawn_wraith(loc: location):
	spawn a wither skeleton at {_loc}
	set name of the last spawned wither skeleton to "&8Wraith"
	
	apply invisibility 1 to the last spawned wither skeleton for 9999 minutes
	apply strength 1 to the last spawned wither skeleton for 9999 minutes
	apply jump boost 2 to the last spawned wither skeleton for 9999 minutes
	
	set tool of last spawned wither skeleton to wooden hoe with name "Wraith Scythe"
	set helmet of last spawned wither skeleton to {skull_wraith}
	set chestplate of last spawned wither skeleton to leather chestplate with name "&cWraith Rags"
	dye chestplate of last spawned wither skeleton (82, 64, 135)
	delete leggings of last spawned wither skeleton
	delete boots of last spawned wither skeleton
	
function spawn_ogre(loc: location):
	spawn a zombie at {_loc}
	set name of the last spawned zombie to "&2Ogre"
	chance of 5%:
		set name of the last spawned zombie to "&2SHREK"
		
	#apply health boost 10 to the last spawned zombie for 9999 minutes
	apply hunger 1 to the last spawned zombie for 9999 minutes
		
	set helmet of last spawned zombie to {skull_ogre}
	set chestplate of the last spawned zombie to leather chestplate with name "&aOgre Chestplate"
	dye the last spawned zombie's chestplate green
	set leggings of the last spawned zombie to leather leggings with name "&aOgre Leggings"
	dye the last spawned zombie's leggings green
	set boots of the last spawned zombie to leather boots with name "&cDemon Boots"
	dye the last spawned zombie's boots green
	
function spawn_elemental(loc: location , etype: number):
	spawn a zombie pigman at {_loc}

	apply resistance 2 to the last spawned zombie pigman for 9999 minutes
	apply slowness 1 to the last spawned zombie pigman for 9999 minutes
	apply invisibility 1 to the last spawned zombie pigman for 9999 minutes
	
	#default elemental is magic

	set name of the last spawned zombie pigman to "&5Magic Elemental"
		
	set tool of the last spawned zombie pigman to blaze rod with name "&5Magic Elemental Staff &k00"
	set helmet of the last spawned zombie pigman to {skull_elemental_ender}
	set chestplate of the last spawned zombie pigman to leather chestplate with name "&5Magic Elemental Robe"
	dye the last spawned zombie pigman's chestplate (110, 0, 255)
	set the last spawned zombie pigman's boots to leather boots with name "&5Elemental boots"
	dye the last spawned zombie pigman's boots (110, 0, 255)
	delete leggings of last spawned zombie pigman
	
	if {_etype} is 1:
		#set name of the last spawned zombie pigman to "&1Water Elemental"
		apply water breathing 1 to the last spawned zombie pigman for 9999 minutes
		set tool of the last spawned zombie pigman to {sword_blue}
		set helmet of the last spawned zombie pigman to {skull_elemental_aqua}
		set chestplate of the last spawned zombie pigman to leather chestplate with name "&3Water Elemental Robe"
		dye the last spawned zombie pigman's chestplate (0, 67, 255)
		dye the last spawned zombie pigman's boots (0, 67, 255)
	else if {_etype} is 2:
		#set name of the last spawned zombie pigman to "&4Magma Elemental"
		apply fire resistance 1 to the last spawned zombie pigman for 9999 minutes
		set tool of the last spawned zombie pigman to {sword_orange}
		set helmet of the last spawned zombie pigman to {skull_elemental_fire}
		set chestplate of the last spawned zombie pigman to leather chestplate with name "&4Magma Elemental Robe"
		dye the last spawned zombie pigman's chestplate (255, 67, 0)
		dye the last spawned zombie pigman's boots (255, 67, 0)
	else if {_etype} is 3:
		#set name of the last spawned zombie pigman to "&2Life Elemental"
		set tool of the last spawned zombie pigman to {sword_green}
		set helmet of the last spawned zombie pigman to {skull_elemental_green}
		set chestplate of the last spawned zombie pigman to leather chestplate with name "&2Life Elemental Robe"
		dye the last spawned zombie pigman's chestplate (13, 186, 13)
		dye the last spawned zombie pigman's boots (13, 186, 13)
	else if {_etype} is 4:
		#set name of the last spawned zombie pigman to "&bFrost Elemental"
		set tool of the last spawned zombie pigman to {sword_white}
		set helmet of the last spawned zombie pigman to {skull_elemental_frost}
		set chestplate of the last spawned zombie pigman to leather chestplate with name "&bFrosts Elemental Robe"
		dye the last spawned zombie pigman's chestplate (71, 188, 198)
		dye the last spawned zombie pigman's boots (71, 188, 198)
	else if {_etype} is 5:
		#set name of the last spawned zombie pigman to "&bDesert Elemental"
		set tool of the last spawned zombie pigman to {sword_sand}
		set helmet of the last spawned zombie pigman to {skull_elemental_sand}
		set chestplate of the last spawned zombie pigman to leather chestplate with name "&eDesert Elemental Robe"
		dye the last spawned zombie pigman's chestplate (186, 143, 65)
		dye the last spawned zombie pigman's boots (186, 143, 65)
	
	#enchant the last spawned zombie pigmans's chestplate with power 1
	#enchant the last spawned zombie pigmans's boots with power 1
	
