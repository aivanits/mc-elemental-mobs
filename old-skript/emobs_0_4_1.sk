function spawn_caravan_trader(loc: location):
	spawn a villager at {_loc}
	set name of the last spawned villager to "Caravan Trader"

function spawn_caravan_guard(loc: location):
	spawn a iron golem at {_loc}
	set name of the last spawned iron golem to "Caravan Guard"
	apply speed 1 to the last spawned iron golem for 3 minutes
	apply resistance 2 to the last spawned iron golem for 3 minutes
	apply strength 5 to the last spawned iron golem for 3 minutes

function spawn_caravan_llama(loc: location):
	spawn a llama at {_loc}
	set name of the last spawned llama to "Caravan Llama"
	
	
